{
 "root": true,
 "parser": "babel-eslint",
 "parserOptions": {
   "ecmaVersion": 6,
   "sourceType": "module",
   "ecmaFeatures": {}
 },
 "env": {
   "browser": true,
   "es6": true,
   "node": true
 },
 "extends": [
   "eslint:recommended",
   "plugin:flowtype/recommended"
 ],
 "plugins": [
   "flowtype"
 ],
 "rules": {
   "no-console": [0],
   "no-unused-vars":"warn",
   "no-undef":"warn",
   "semi": [2, "never"]
 }
}
